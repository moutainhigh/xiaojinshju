    line-height: 30px;
    display: inline-block;
    vertical-align: middle;
}
.valid-form{
    position: relative;
}
.valid-form-item{
    position: absolute;
    font-size: 14px;
    top: -29px;
    left: 192px;
    z-index: 90;
}
.valid-form-item > div > span {
    width: 280px;
    height: 28px;
    background: #ffffff;
    position: absolute;
