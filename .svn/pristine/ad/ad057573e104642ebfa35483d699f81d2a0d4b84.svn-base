}

.input-group,.input-checkbox-group{
    width: 425px;
    margin: 0 auto 20px;
    position: relative;
}
.input-group label,.input-checkbox-group > label{
    color: #4D4D4D;
    width: 85px;
    text-align: right;
    margin-right: 19px;
    font-size: 14px;
}
.input-checkbox-group > div{
    display: inline-block;
}
.input-checkbox-group > div > label{
    padding-top: 0px!important;
}
.input-group input,.input-group select{
    width: 291px;
    height: 34px;
    border-radius: 2px;
    height: 34px;
    padding: 6px 12px;
    font-size: 14px;
    line-height: 1.42857143;
    color: #555;
    background-color: #fff;
    background-image: none;
    border: 1px solid #ccc;
    -webkit-transition: border-color ease-in-out .15s,-webkit-box-shadow ease-in-out .15s;
    -o-transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
    transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s
}


.input-group input:focus,.input-group select:focus {
    border-color: #4777B6;
}


.input-btn-group{
    width: 291px;
    margin: 0 auto;
    padding-left: 37px;
}
.input-btn-group > button{
    width: 291px;
    height: 38px;
    background: #4777B6;
    border-radius: 2px;
    color: #ffffff;
    border:none;
}
.input-btn-group > button:hover{
    color: #ffffff;
}
.input-btn-group > div > button{
    width: 291px;
    height: 38px;
    border-radius: 2px;
    color: #ffffff;
}
.input-btn-group > div > p{
    width: 291px;
    color: #999999;
    font-size: 14px;
    margin-top: 23px;
    text-align: center;
}
.input-btn-group > div > button:hover{
    color: #ffffff;
}
