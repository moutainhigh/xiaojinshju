.ellipsis-hide{
    position: relative;
    cursor: pointer;
}
.ellipsis-hide > div{
    position: absolute;
    width: max-content;
    top: 0px;
    left: 156px;
    padding: 10px;
    background: rgba(0,0,0,.8);
    color: white;
    z-index: 10;
}
.ellipsis-hide > div >div{

}
.ellipsis-hide > div > span{
    display: block;
    position: absolute;
    left: -16px;
    top: 10px;
    width: 0px;
    height: 0px;
    border-top: 8px solid transparent;
    border-right: 16px solid rgba(0,0,0,.8);
    border-bottom: 8px solid transparent;
}
