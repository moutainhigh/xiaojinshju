.queryProgressBar{
    position: absolute;
    top: 0px;
    left: 0;
    width: 100%;
    height: 6px;
}
.level_GREAT{
    color:#00DF63;
    cursor: pointer;
}
.level_GOOD{
    color:#FCCC00;
    cursor: pointer;
}
.level_BAD{
    color:#FF2928;
    cursor: pointer;
}
.level_null{
    color:#AAA6A2;
    cursor: pointer;
}

.animation{
    transition: width .5s;
}
